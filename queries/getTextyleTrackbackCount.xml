<query id="getTextyleTrackbackCount" action="select">
    <tables>
        <table name="trackbacks" />
    </tables>
    <columns>
        <column name="module_srl" />
        <column name="count(*)" alias="count" />
    </columns>
    <conditions>
        <condition operation="in" column="module_srl" var="module_srls" filter="numbers" notnull="notnull" pipe="and" />
    </conditions>
    <groups>
        <group column="trackbacks.module_srl" />
    </groups>
</query>
